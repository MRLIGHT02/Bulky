@model List<Category>

<h1>
	Categrory List
</h1>
<table class="table table-bordered table-striped">
	<thead>

		<tr>
			<th>Category Name</th>
			<th>
				Display Order
			</th>
			<th>Category Id</ th>
		</tr>
	</thead>
	@foreach(var obj in Model)
	{
		

	<tr>
		<td>@obj.Name</td>
		<td>@obj.DisplayOrder</td>
		<td>@obj.CatagoryId</td>
	</tr>
	
	}
</table>